<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <rotate
      android:fromDegrees="90"
      android:toDegrees="90">
      <shape android:shape="line">
        <stroke
          android:width="1dp"
          android:color="?tink_textColorPrimary"
          android:dashWidth="1dp"
          android:dashGap="3dp" />
      </shape>
    </rotate>
  </item>
</selector>